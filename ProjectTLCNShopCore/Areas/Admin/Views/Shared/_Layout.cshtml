@using Microsoft.AspNetCore.Http
<!DOCTYPE html>
<html lang="en">
<head>
	<!-- start: Meta -->
	<meta charset="utf-8">
	<title>Managerment Online Shop Electronic</title>
	<!-- start: Mobile Specific -->

	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- end: Mobile Specific -->

	<!-- start: CSS -->
	<link id="bootstrap-style" href="~/css/bootstrap.min.css" rel="stylesheet">
	<link id="base-style" href="~/css/style.css" rel="stylesheet">
	<link href="~/css/bootstrap-responsive.min.css" rel="stylesheet">
	<link id="base-style-responsive" href="~/css/style-responsive.css" rel="stylesheet">
	<!-- end: CSS -->
	<!-- start: Favicon -->
	<link rel="shortcut icon" href="favicon.ico">
	<!-- end: Favicon -->
	<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/jasny-bootstrap/3.1.3/css/jasny-bootstrap.min.css" />

</head>

<body>
	<!-- start: Header -->
	<div class="navbar">
		<div class="navbar-inner">
			<div class="container-fluid">
				<a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>
				<a class="brand" href="index.html">Shop Computer</a>

				<!-- start: Header Menu -->
				<div class="nav-no-collapse header-nav">
					<ul class="nav pull-right">
						
						<!-- start: User Dropdown -->
						<li class="dropdown">
							<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
								<i class="halflings-icon white user"></i> Thanh An Nguyen
								<span class="caret"></span>
							</a>
							<ul class="dropdown-menu">
								<li class="dropdown-menu-title">
									<span>Account Settings</span>
								</li>
								@if (Context.Session.GetString("UserID") != null)
								{								
									<li><a href="@Url.Action("Logout","Login")" ><i class="halflings-icon off"></i> Logout</a></li>
								}
								
							</ul>
						</li>
						<!-- end: User Dropdown -->
					</ul>
				</div>
				<!-- end: Header Menu -->

			</div>
		</div>
	</div>
	<!-- start: Header -->

	<div class="container-fluid-full">
		<div class="row-fluid">

			<!-- start: Main Menu -->
			<div id="sidebar-left" class="span2">
				<div class="nav-collapse sidebar-nav">
					<ul class="nav nav-tabs nav-stacked main-menu">
						<li>
							<a href="index.html">
								<i class="icon-dashboard"></i><span class="hidden-tablet"> Dashboard</span>
							</a>
						</li>
						<li>
							<a href="tinnhan.html">
								<i class="icon-envelope"></i><span class="hidden-tablet"> Tin nhắn</span>
							</a>
						</li>
						<li>
						<li>
							<a href="thongbao.html">
								<i class="icon-eye-open"></i><span class="hidden-tablet"> Thông báo</span>
							</a>
						</li>
						<li>
							<a href="/Admin/Products">
								<i class="icon-barcode"></i><span class="hidden-tablet"> Sản phẩm</span>
							</a>
						</li>

						<li>
							<a href="donhang.html">
								<i class="icon-edit"></i><span class="hidden-tablet"> Đơn hàng</span>
							</a>
						</li>
						<li>
							<a href="/Admin/Categorie">
								<i class="icon-list-alt"></i><span class="hidden-tablet"> Loại sản phẩm</span>
							</a>
						</li>
						<li>
							<a href="/Admin/Suppliers">
								<i class="icon-briefcase"></i><span class="hidden-tablet"> Nhà cung cấp</span>
							</a>
						</li>
						<li>
							<a href="/Admin/Shippers">
								<i class="icon-truck"></i><span class="hidden-tablet"> Đơn vị vận chuyển</span>
							</a>
						</li>

						<li>
							<a href="#">
								<i class="icon-calendar"></i><span class="hidden-tablet"> Calendar</span>
							</a>
						</li>
						<li>
							<a href="quanlytrangchu.html">
								<i class="icon-folder-open"></i><span class="hidden-tablet"> Quản lý trang chủ</span>
							</a>
						</li>

						<li>
							<a href="/Admin/Users">
								<i class="icon-lock"></i><span class="hidden-tablet">Quản Lý Tài khoản</span>
							</a>
						</li>
					</ul>
				</div>
			</div>
		</div>

		@RenderBody()

		<!-- end: Main Menu -->

	</div><!--/fluid-row-->


	<div class="clearfix"></div>

	<footer>

		<p>
			<span style="text-align:left;float:left">&copy; 2016 <a href="#" alt="Bootstrap_Metro_Dashboard">HCMUTE Metro</a></span>

		</p>

	</footer>

	<!-- start: JavaScript-->

	<script src="~/myjs/scripttemplate/jquery-1.9.1.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery-migrate-1.0.0.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery-ui-1.10.0.custom.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.ui.touch-punch.js"></script>
	<script src="~/myjs/scripttemplate/modernizr.js"></script>
	<script src="~/myjs/scripttemplate/bootstrap.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.cookie.js"></script>
	<script src="~/myjs/scripttemplate/fullcalendar.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.dataTables.min.js"></script>
	<script src="~/myjs/scripttemplate/excanvas.js"></script>
	<script src="~/myjs/scripttemplate/jquery.flot.js"></script>
	<script src="~/myjs/scripttemplate/jquery.flot.pie.js"></script>
	<script src="~/myjs/scripttemplate/jquery.flot.stack.js"></script>
	<script src="~/myjs/scripttemplate/jquery.flot.resize.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.chosen.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.uniform.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.cleditor.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.noty.js"></script>
	<script src="~/myjs/scripttemplate/jquery.elfinder.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.raty.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.iphone.toggle.js"></script>
	<script src="~/myjs/scripttemplate/jquery.uploadify-3.1.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.gritter.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.imagesloaded.js"></script>
	<script src="~/myjs/scripttemplate/jquery.masonry.min.js"></script>
	<script src="~/myjs/scripttemplate/jquery.knob.modified.js"></script>
	<script src="~/myjs/scripttemplate/jquery.sparkline.min.js"></script>
	<script src="~/myjs/scripttemplate/counter.js"></script>
	<script src="~/myjs/scripttemplate/retina.js"></script>
	<script src="~/myjs/scripttemplate/custom.js"></script>

	<!-- end: JavaScript-->
	@RenderSection("scripts", required: false)
</body>
</html>
